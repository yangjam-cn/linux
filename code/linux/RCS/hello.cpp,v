@/* $Id: hello.cpp,v 1.5 2020/08/30 12:33:03 youngjam Exp youngjam $
 */
#include <iostream>
#include <cstdlib>
#include <unistd.h>
static char rcsid[] = "$Id$\n";
int main(void)
{
    using namespace std;

    extern char **environ;
    char **my_env = environ;
    while (*my_env != nullptr)
    {
        cout << *my_env << endl;
        my_env++;
    }

    cout << "hello , linux!\n" << "test g++." << endl;
    cout << "hello RCS!" << endl;
    return 0;
}@


