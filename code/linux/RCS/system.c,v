@#include <stdlib.h>
#include <stdio.h>

int main(void)
{
    int retval;

    retval = system("ls -l");

    if (127 == retval)    /*bin/sh没找到*/
    {
        fprintf(stderr, "/bin/sh not available\n");
        exit(127);
    }
    else if (-1 == retval)   /*函数执行出错*/
    {
        perror("system");
        exit(EXIT_FAILURE);
    }
    else if (0 != retval)    /*system参数传递为NULL*/
    {
        fprintf(stderr, "command returned %d\n", retval);
        perror("ls");
    }
    else
    {
        puts("command successfully executed");
    }
    exit(EXIT_SUCCESS);
}@
