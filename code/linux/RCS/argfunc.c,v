text
@#include <stdio.h>
void test(int a, int b)
{
    printf("%d\t%d\n", a, b);
}

int main(void)
{
    int i = 0;
    test(i++, i++);    /*out: 1 0*/
    /*先执行第二个i++,将参数传递给子函数后,再执行第一个*/
    i = 0;
    test(++i, ++i);    /*out: 2 2*/
    /*等++i均执行完后在传递参数*/
    i = 0;
    test(i++, ++i);   /*out: 1 2*/
    i = 0;
    test(++i, i++);   /*out: 2 0*/
    return 0;
}@
